      <TextField fx:id="FirstName" layoutX="226.0" layoutY="134.0" promptText="FirstName" />
      <TextField fx:id="Password" layoutX="226.0" layoutY="227.0" promptText="Password" />
      <TextField fx:id="Email" layoutX="226.0" layoutY="273.0" promptText="Email" />
      <Button fx:id="RegisterButton" layoutX="247.0" layoutY="322.0" mnemonicParsing="false" onAction="#register" text="Register Account" />
